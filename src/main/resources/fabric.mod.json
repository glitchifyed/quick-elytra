{
  "schemaVersion": 1,
  "id": "quick_elytra",
  "version": "${version}",

  "name": "Quick Elytra",
  "description": "Quick Elytra adds a keybind that lets you swap your equipped elytra/chestplate. It prioritises equipment with the most enchantments/enchantment levels and durability.",
  "authors": [
    "glitchifyed"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/quick-elytra",
    "sources": "https://github.com/glitchifyed/quick-elytra",
    "issues": "https://github.com/glitchifyed/quick-elytra/issues"
  },

  "license": "LGPL-3.0",
  "icon": "assets/quick_elytra/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.glitchifyed.quick_elytra.QuickElytra"
    ],
    "client": [
      "net.glitchifyed.quick_elytra.QuickElytraClient"
    ]
  },
  "mixins": [
    "quick_elytra.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
